  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node src/server",
    "lint": "eslint src",
    "test:unit:client": "NODE_ENV=test jest src/client --forceExit --detectOpenHandles --coverage --verbose",
    "test:unit:server": "NODE_ENV=test jest src/client --forceExit --detectOpenHandles --coverage --verbose",
    "test:unit": "npm run test:unit:client && npm run test:unit:server",
    "test": "npm run test:unit",
    "build:server": "babel --out-dir build --ignore __tests__ --ignore _e2e --copy-files src/",
    "build:client": "webpack --config config/webpack.config.prod.js -p --mode production",
    "build": "npm run build:server && npm run build:client",
    "serve-prod": "sls offline start",
    "package": "sls package",
    "deploy": "sls deploy"
  },
